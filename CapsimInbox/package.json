{
  "name": "capsiminbox",
  "version": "0.1.0",
  "description": "Capsim Inbox",
  "author": "Paulius Juskevicius & Javier Cohen",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --ignore public/ --ignore static/ --inspect ./www",
    "watchify:studentportal": "watchify -vd -p browserify-hmr -e static/studentPortal/main.js -o public/javascripts/build.studentportal.js",
    "watchify:prof": "watchify -vd -p browserify-hmr -e static/professor/main.js -o public/javascripts/build.professor.js",
    "watchify:assessment": "watchify -vd -p browserify-hmr -e static/assessment/main.js -o public/javascripts/build.assessment.js",
    "watchify:admin": "watchify -vd -p browserify-hmr -e static/admin/main.js -o public/admin/javascripts/build.js",
    "build:assessment": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o public/stylesheets/build.assessment.css ] -e static/assessment/main.js | uglifyjs -c warnings=false -m > public/javascripts/build.assessment.js",
    "build:studentportal": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o public/stylesheets/build.studentportal.css ] -e static/studentPortal/main.js | uglifyjs -c warnings=false -m > public/javascripts/build.studentportal.js",
    "build:prof": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o public/stylesheets/build.professor.css ] -e static/professor/main.js | uglifyjs -c warnings=false -m > public/javascripts/build.professor.js",
    "build:admin": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o public/admin/stylesheets/build.css ] -e static/admin/main.js | uglifyjs -c warnings=false -m > public/admin/javascripts/build.js",
    "dev:assessment": "npm-run-all --parallel start watchify:assessment",
    "dev:prof": "npm-run-all --parallel start watchify:prof",
    "dev:studentportal": "npm-run-all --parallel start watchify:studentportal",
    "dev:admin": "npm-run-all --parallel start watchify:admin",
    "build:all": "npm run build:prof & npm run build:admin & npm run build:assessment & npm run build:studentportal",
    "build:env": "node buildEnv.js"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "ajv": "^6.12.3",
    "body-parser": "~1.15.2",
    "bookshelf": "^0.10.4",
    "bookshelf-cascade-delete": "^2.0.1",
    "connect-redis": "^3.2.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.14.0",
    "express-session": "^1.15.2",
    "hbs": "~4.0.1",
    "helmet": "^3.9.0",
    "json2csv": "^3.11.1",
    "knex": "^2.4.0",
    "lodash.once": "^4.1.1",
    "moment": "^2.18.1",
    "morgan": "~1.9.1",
    "mssql": "3.3",
    "node-ssh": "^5.0.0",
    "nodemailer": "^4.0.1",
    "passport": "^0.6.0",
    "passport-ldapauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "phantom-html-to-pdf": "^0.5.4",
    "phantomjs-prebuilt": "^2.1.15",
    "serve-favicon": "~2.3.0",
    "uuid": "^3.1.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "bootstrap-sass": "^3.3.7",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.4.0",
    "cross-env": "^2.0.0",
    "date-fns": "^1.28.5",
    "downloadjs": "^1.4.7",
    "envify": "^3.4.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "highcharts": "^9.0.0",
    "highcharts-solid-gauge": "^0.1.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "lodash.debounce": "^4.0.8",
    "lodash.delay": "^4.1.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "tinymce": "^5.10.7",
    "uglify-js": "^2.5.0",
    "vue": "^2.3.2",
    "vue-bootstrap-datetimepicker": "^4.1.2",
    "vue-router": "3.0.1",
    "vue-slider-component": "https://registry.npmjs.org/vue-slider-component/-/vue-slider-component-2.3.1.tgz",
    "vuedraggable": "^2.16.0",
    "vueify": "^9.0.0",
    "vuex": "^2.3.1",
    "watchify": "^3.4.0"
  }
}
